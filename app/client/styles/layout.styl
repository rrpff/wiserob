@import url('https://fonts.googleapis.com/css?family=Ubuntu:400,700');

body
  background #F4D03F
  font-family Ubuntu, sans-serif
  font-weight 400
  overflow none

.wrapper
  display block
  height 100%
  width 100%
  left 0
  top 0
  position absolute
  overflow-y auto

.centerWrapper
  position relative
  margin 100px auto
  width 800px
  max-width 100%
